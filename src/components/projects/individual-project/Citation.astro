---
import Link from "../../Link.astro";
const { citationLink, citationText, githubLink } = Astro.props;
---

{
  citationLink && citationText ? (
    <Link href={citationLink} text={citationText} />
  ) : citationText ? (
    <p>{citationText}</p>
  ) : (
    <Link href={githubLink} text={"Cite the GitHub repository"} />
  )
}
