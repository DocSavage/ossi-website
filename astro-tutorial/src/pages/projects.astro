---
import BaseLayout from '../layouts/BaseLayout.astro';

const allPosts = await Astro.glob('../pages/projects/*.md')
const pageTitle = "OSSI-supported projects"

---
<BaseLayout pageTitle={pageTitle}>
	<p>Additional text about projects</p>
    <ul>
        {allPosts.map((post) => (
            <li><a href={post.url}>{post.frontmatter.title}</a></li>
        ))}
       </ul>
</BaseLayout>
